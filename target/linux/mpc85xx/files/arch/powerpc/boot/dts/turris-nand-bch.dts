/include/ "turris.dtsi"

/ {
	lbc: localbus@ffe05000 {
		nand@1,0 {
			nand-ecc-mode = "soft_bch";
			nand-ecc-step-size = <512>;
			nand-ecc-strength = <4>; 

		};
	};
};

